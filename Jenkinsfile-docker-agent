//copied from https://github.com/darinpope/jenkins-example-docker/blob/main/Jenkinsfile-1
pipeline {
  agent {
    //docker { image 'linuxdockeragent_v3-agent:latest' }
    docker { image 'my-java-image:latest' }
  }
  stages {
    stage('Pre-test') {
      steps {
         sh 'echo "----Pre--------"'
         sh 'hostname'
          sh 'echo "---Pre---------"'
      }
    }
    stage('Test') {
      steps {
         sh 'echo "----Test--------"'
         sh 'hostname'
          sh 'whoami'
          sh 'echo $PATH'
          sh 'echo "---Test---------"'
          //sh 'java --version'
          sh 'node --version'
         // sh 'curl --version'
        //  sh 'which nano'
        //  sh 'ping -c 4 jenkins'
          
      }
    }
  }
}
