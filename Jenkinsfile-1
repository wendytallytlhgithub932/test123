pipeline {
   agent {
    docker { image 'node:16-alpine' }
    user 'jenkins' //added by wendy //added a jenkins user to the Jenkins. This worked after DnD setup and run this in the pipeline box on the Jenkins
   }
    stages {
        stage('Checkout') {
            steps {
                sh 'git config user.name ${GIT_USER_NAME}'
                sh 'git config user.email ${GIT_USER_EMAIL}'
                sh 'git clone https://github.com/darinpope/jenkins-example-docker.git'
            }
        }
        // Add other stages here
      stage('Test') {
        steps {
          sh 'node --version'
        }
      }
    }
}
