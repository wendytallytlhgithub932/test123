pipeline {
    agent any
    environment {
        GIT_USER_NAME = credentials('GIT_USER_NAME')
        GIT_USER_EMAIL = credentials('GIT_USER_EMAIL')
    }
    stages {
        stage('Checkout') {
            steps {
                sh 'git config user.name ${GIT_USER_NAME}'
                sh 'git config user.email ${GIT_USER_EMAIL}'
                sh 'git clone https://github.com/darinpope/jenkins-example-docker.git'
            }
        }
        // Add other stages here
      stage('Test') {
        steps {
          sh 'node --version'
        }
      }
    }
}
